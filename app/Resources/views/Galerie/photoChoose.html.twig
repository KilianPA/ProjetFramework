
{#{{ dump(photos[0][0].nom) }}#}
{#{{ photos|length }}#}
    <div class="col-md-12">
        <h2>Choisir l'ordre</h2>
    </div>
        {% for photo in photos %}
            <div class="col-md-3 photo_case">
                <div class="col-md-12 photo_gridchoose_each" photoid="{{ photo[0].id }}">
                    <div class="col-md-12 validate_icon text-center">
                        <i class="fa fa-check-circle valider_click_photo" aria-hidden="true"></i>
                    </div>
                    <img src="uploads/photo/{{ photo[0].path }}" class="img-responsive image_photo_grid">
                </div>
                <div class="col-md-12 container_input_ordre ">
                    <div class="col-md-8 center-block container_input">
                        <input class="form-control order_choose" type="number" value="" id="example-number-input" placeholder="1">
                    </div>
                </div>
            </div>
       {% endfor %}