
<div class="col-md-12 container_galerie_home">
    <div class="col-md-12">
        <h3> "{{  galeries.name }}" de <a class="username_home"> @{{  galeries.utilisateur.username }} </a> </h3>
        <hr class="ligne_home">
    </div>
    {% for photo in photos %}
        <div class="col-md-3 container_image_home">
            <div class="col-md-12 text-center">
                <h3 class="name_photo"> {{ photo.nom }} </h3>
            </div>
            <img src="uploads/photo/{{ photo.path }}" class="img-responsive image_photo_home">
        </div>
    {% endfor %}
</div>